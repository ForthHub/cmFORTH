	 A
	 0 DO -GAMME LOOP ;

: REKLAME
	3 /32 F +OCT BES -OCT	    \ C1 F1
	1 /32 G +OCT A	 -OCT G	    \ D1 E1 D1
	1 /16 F			    \ C1
	1 /32 ES		    \ BS1
	3 /32 F
	1 /32 BES C  D		    \ F0 G0 A1
	1 /4  F			    \ C1
	;

